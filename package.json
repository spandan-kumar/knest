{
  "name": "knest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -t knest .",
    "docker:build:node": "docker build -f Dockerfile.node -t knest-node .",
    "docker:run": "docker run -p 3000:3000 --env-file .env knest",
    "docker:run:node": "docker run -p 3000:3000 --env-file .env knest-node",
    "docker:compose": "docker-compose up --build",
    "docker:compose:down": "docker-compose down"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "lamejs": "^1.2.1",
    "next": "15.4.4",
    "next-pwa": "^5.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.18",
    "cssnano": "^7.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
